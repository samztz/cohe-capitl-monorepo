# Production Environment Variables
# Copy this to .env.production for production deployment

# API Base URL (Production backend)
NEXT_PUBLIC_API_BASE=https://api.cohe.capital

# WalletConnect Project ID (from https://cloud.reown.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_production_project_id_here

# SIWE Configuration (Production)
NEXT_PUBLIC_SIWE_DOMAIN=cohe.capital
NEXT_PUBLIC_SIWE_URI=https://api.cohe.capital

# BSC Mainnet (Chain ID 56 for production)
NEXT_PUBLIC_CHAIN_ID=56
