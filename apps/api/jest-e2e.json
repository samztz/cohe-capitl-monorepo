{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testRegex": ".e2e.spec.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "collectCoverageFrom": ["**/*.ts"],
  "coverageDirectory": "./coverage-e2e",
  "testEnvironment": "node",
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1"
  },
  "transformIgnorePatterns": ["/node_modules/", "/generated/"]
}
